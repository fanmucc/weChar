.cart-list {
    display: flex;
    width: 100%;
    padding: 10rpx;
    box-sizing: border-box;
    border-bottom: 0.5px solid rgba(231,231,231,1);
    background: #fff;
}
.images {
    width: 200rpx;
    height: 200rpx; 
    background-color: #eeeeee;
}
.cart-list-content {
    padding-left: 20rpx;
}
.cart-list-content .content-box {
    height: 33.3%;
}
.content-box {
    display: flex;
    align-items: center;
}
.cart-name {
    font-size: 32rpx;
}
.cart-num {
    font-size: 28rpx;
    color: #818181;
}
.cart-color {
    font-size: 28rpx;
    color: #818181
}