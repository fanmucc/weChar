@import '/assets/css/iconfont/iconfont.wxss';
.costomBar {
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    align-items: center;
    width: 100%;
    height: 47px;
    border-top: 1px solid #d5d5d5;
    background: white;
}
.costomBar .costomTab {
    flex: 1;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    position: relative;
}
.tabBar-text {
    font-size: 11px;
    font-weight: 400;
}

.tabBar-icon {
    font-size: 20px;
    margin-bottom: 1px;
}

.color {
    color: #9BD0EA
}

.empty {
    height: 47px;
}

